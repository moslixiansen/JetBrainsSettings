<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Diff.ApplyLeftSide" />
  <action id="Diff.ApplyRightSide" />
  <action id="EditorSelectWord" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift ctrl back_quote" />
    <keyboard-shortcut first-keystroke="shift alt numpad1" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="shift alt numpad2" />
  </action>
  <action id="Git.Stash">
    <keyboard-shortcut first-keystroke="shift alt page_up" />
  </action>
  <action id="Git.Unstash">
    <keyboard-shortcut first-keystroke="shift alt page_down" />
  </action>
  <action id="ManageRecentProjects">
    <keyboard-shortcut first-keystroke="shift e" />
  </action>
  <action id="NextEditorTab" />
  <action id="PreviousEditorTab" />
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt s" />
    <keyboard-shortcut first-keystroke="shift ctrl alt s" />
  </action>
  <action id="Vcs.Push">
    <keyboard-shortcut first-keystroke="shift ctrl k" />
    <keyboard-shortcut first-keystroke="shift alt numpad3" />
  </action>
</keymap>